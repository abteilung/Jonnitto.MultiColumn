prototype(Jonnitto.ImagesInARow:Flex) < prototype(Fusion:Array) {
	asset = ${asset ? asset : false}
	relation = ${this.asset.width / this.asset.height}
	@ignoreProperties = ${['asset', 'relation']}

	@if.hasAsset = ${this.asset ? true : false}

	webkitBoxFlex = ${'-webkit-box-flex:' + this.relation + ';'}
	webkitFlex = ${'-webkit-flex:' + this.relation + ' 1 0%;'}
	msFlex = ${'-ms-flex:' + this.relation + ' 1 0%;'}
	flex = ${'flex:' + this.relation + ' 1 0%;'}
}
